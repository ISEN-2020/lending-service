[run]
source = .
omit = 
    */venv/*
    */migrations/*
    */tests/*
    */test_*
    manage.py
    */settings.py
    */wsgi.py
    */asgi.py
    */__pycache__/*
    */static/*
    */media/*
    check_compliance.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\(Protocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml